{
  "name": "three-glslify-sketch-boilerplate",
  "version": "1.0.0",
  "description": "a boilerplate which makes use of modern js tooling, gives convenience to a stackgl modules using glslify, and a three.js harnes for quick prorotyping",
  "main": "index.js",
  "author": "Tyler Powers / @gr4yscale",
  "license": "ISC",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "olddevserver": "budo js/index.js:build/bundle.js -t glslify -v --live --host=192.168.56.101 | garnish",
    "oldbuild": "browserify -t glslify js/index.js | uglifyjs -cm > build/bundle.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
