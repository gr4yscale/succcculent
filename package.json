    "lodash": "^4.17.4",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-redux": "^5.0.3",
    "react-scripts": "0.8.5",
    "redux": "^3.6.0",
    "redux-devtools": "^3.3.2",
    "redux-devtools-chart-monitor": "^1.6.1",
    "redux-devtools-dock-monitor": "^1.1.1",
